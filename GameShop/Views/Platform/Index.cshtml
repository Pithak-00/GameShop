@model List<Platform>

<h1>Platform List</h1>
<table class=" table table-bordered table-striped">
	<thead>
		<tr>
			<th>
				Platform Name
			</th>
			<th>
				Display Order
			</th>
		</tr>
	</thead>
	<tbody>
		<!-- Platform情報表示 -->
		@foreach(var obj in Model.OrderBy(u=>u.DisplayOrder))
		{
			<tr>
				<td>@obj.Name</td>
				<td>@obj.DisplayOrder</td>
			</tr>
		}
	</tbody>
</table>